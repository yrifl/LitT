{
  "name": "LitT",
  "tree": {
    "$className": "DataModel",

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",
      "Launch": {
        "$path": "src/Temp"
      }
    },

    "ReplicatedStorage": {
      "Common": {
        "$path": "src/Shared"
      },
      "Packages": {
        "$className": "Folder",
        "$path": "Packages/"
      }
    },

    "ServerScriptService": {
      "$properties": {
        "LoadStringEnabled": true
      },
      "$className": "ServerScriptService",
      "$path": "src/Server"
    },

    "StarterPlayer": {
      "$properties": {
        "CharacterWalkSpeed": 16
      },
      "StarterPlayerScripts": {
        "$path": "src/Client"
      }
    },

    "Workspace": {
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Size": [
            512.0,
            20.0,
            512.0
          ],
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Position": [
            0.0,
            -10.0,
            0.0
          ],
          "Locked": true
        }
      },
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Technology": "Voxel",
        "Outlines": false,
        "GlobalShadows": true,
        "Ambient": [
          0.0,
          0.0,
          0.0
        ],
        "Brightness": 2.0
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  },
  "globIgnorePaths": [
    "**/*.spec.lua"
  ]
}